package br.com.caicai.teste;

import javax.swing.JOptionPane;

import br.com.caicai.modelo.Elevador;

public class TesteElevador {
	
		
public static void main(String[] args) {
		
		
		
		//Instanciando um objeto
			Elevador caicai = new Elevador();
				
			caicai.pNome(JOptionPane.showInputDialog("Nome: "));
			caicai.pMaxPessoas((byte)15);
			caicai.pAndarMax((byte)15);
			caicai.pAndarMin((byte)0);
			
			
			System.out.println("Saida :" + caicai.retornarTudo());
				
		
		
		
			
		
		
		
		
		
		
		
		
		
		
		
	}
	
	
	
	
	
	
	
	

}
